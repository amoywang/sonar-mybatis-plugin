<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 基础层：消息接收方-Mapper -->
<mapper namespace="com.cndinfo.cndlct.ssp.repository.impl.message.mapper.MessageReceiverPOMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cndinfo.cndlct.ssp.repository.impl.message.po.MessageReceiverPO">
        <id column="id" property="id"/>
